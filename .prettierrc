{
  "semi": true,
  "singleQuote": true,
  "endOfLine": "auto",
  "printWidth": 100,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": [".css$", "<THIRD_PARTY_MODULES>", "^@abrplus", "^[$/]", "^[./]"],
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": ["*.json5"],
      "options": {
        "singleQuote": false,
        "quoteProps": "preserve"
      }
    },
    {
      "files": ["*.yml"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
