{
  "name": "abrplus-ui-kit",
  "version": "0.3.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "style": "./dist/index.css",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./atoms": {
      "types": "./dist/types/atoms.d.ts",
      "import": "./dist/atoms.es.js",
      "require": "./dist/atoms.cjs.js"
    },
    "./molecules": {
      "types": "./dist/types/molecules.d.ts",
      "import": "./dist/molecules.es.js",
      "require": "./dist/molecules.cjs.js"
    },
    "./organisms": {
      "types": "./dist/types/organisms.d.ts",
      "import": "./dist/organisms.es.js",
      "require": "./dist/organisms.cjs.js"
    },
    "./ant-overrides": {
      "types": "./dist/types/antOverrides.d.ts",
      "import": "./dist/antOverrides.es.js",
      "require": "./dist/antOverrides.cjs.js"
    },
    "./provider": {
      "types": "./dist/types/provider.d.ts",
      "import": "./dist/provider.es.js",
      "require": "./dist/provider.cjs.js"
    },
    "./style.css": "./dist/index.css",
    "./theme.css": "./dist/theme.css"
  },
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write --cache . --plugin=prettier-plugin-organize-imports",
    "preview": "vite preview",
    "compile": "tsc --excludeFiles vite.config.ts",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "gen:icon": "cross-env SKIP_HUSKY_TASKS=true tsx scripts/icon.ts --commit",
    "gen:tw": "tsx scripts/tailwind-config.ts",
    "build": "pnpm format && pnpm lint && pnpm test && pnpm compile && vite build",
    "build:icon": "pnpm gen:icon && pnpm build",
    "release": "echo //registry.npmjs.org/:_authToken=$PUBLISH_TOKEN > .npmrc && semantic-release && rimraf .npmrc",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@use-gesture/react": "^10.3.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "jalaali-react-date-picker": "git+https://github.com/1stdevelopment/jalaali-react-date-picker.git",
    "react-copy-to-clipboard": "5.1.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook-addon-faker/addon": "^0.5.4",
    "@storybook/addon-actions": "7.6.10",
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/addon-interactions": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/blocks": "7.6.10",
    "@storybook/react": "7.6.10",
    "@storybook/react-vite": "7.6.10",
    "@storybook/test": "7.6.10",
    "@storybook/testing-react": "^2.0.1",
    "@tanstack/react-query": "^5.55.4",
    "@tanstack/react-query-devtools": "5.56.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^14.3.1",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/moment-jalaali": "^0.7.8",
    "@types/node": "^20.10.1",
    "@types/qs": "6.9.7",
    "@types/react": "^18.2.39",
    "@types/react-copy-to-clipboard": "^5.0.5",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-basic-ssl": "1.0.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitejs/release-scripts": "1.2.1",
    "@vitest/coverage-v8": "4.0.16",
    "@vitest/ui": "4.0.16",
    "autoprefixer": "^10.4.16",
    "chalk": "5.3.0",
    "classnames": "2.3.2",
    "commander": "11.1.0",
    "cross-env": "7.0.3",
    "cssnano": "6.0.1",
    "dotenv": "16.3.1",
    "eslint": "8.56.0",
    "eslint-define-config": "1.23.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-regexp": "1.15.0",
    "eslint-plugin-storybook": "^0.6.15",
    "fast-glob": "3.3.2",
    "husky": "9.1.6",
    "jsdom": "22.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "0.5.6",
    "qs": "6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.4",
    "react-is": "18.2.0",
    "react-router-dom": "^6.16.0",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^6.0.5",
    "semantic-release": "^25.0.3",
    "storybook": "7.6.10",
    "storybook-react-context": "^0.7.0",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "tslib": "2.6.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.0",
    "vite-plugin-clean": "^2.0.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "antd": "5.19.1",
    "i18next": "^23",
    "moment-jalaali": "^0.10.0",
    "react": ">=18",
    "react-dom": ">=18",
    "react-i18next": "^13",
    "react-router-dom": "^6",
    "starkstring": "*",
    "tailwind-merge": "^1.14"
  },
  "peerDependenciesMeta": {
    "react-router-dom": {
      "optional": true
    },
    "i18next": {
      "optional": true
    },
    "react-i18next": {
      "optional": true
    },
    "tailwind-merge": {
      "optional": true
    },
    "starkstring": {
      "optional": true
    },
    "antd": {
      "optional": true
    },
    "moment-jalaali": {
      "optional": true
    }
  }
}
